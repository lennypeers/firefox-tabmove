{
  "manifest_version": 2,
  "name": "tabmove",
  "version": "1.0",
  "description": "move current tab with alt+shit+n",
  "permissions": [
      "tabs",
      "activeTab"
  ],
  "commands": {
    "go-to-1": {
      "description": "go to tab n1",
      "suggested_key": {
        "default": "Alt+Shift+1"
      }
    },
    "go-to-2": {
      "description": "go to tab n2",
      "suggested_key": {
        "default": "Alt+Shift+2"
      }
    },
    "go-to-3": {
      "description": "go to tab n3",
      "suggested_key": {
        "default": "Alt+Shift+3"
      }
    },
    "go-to-4": {
      "description": "go to tab n4",
      "suggested_key": {
        "default": "Alt+Shift+4"
      }
    },
    "go-to-5": {
      "description": "go to tab n5",
      "suggested_key": {
        "default": "Alt+Shift+5"
      }
    },
    "go-to-6": {
      "description": "go to tab n6",
      "suggested_key": {
        "default": "Alt+Shift+6"
      }
    },
    "go-to-7": {
      "description": "go to tab n7",
      "suggested_key": {
        "default": "Alt+Shift+7"
      }
    },
    "go-to-8": {
      "description": "go to tab n8",
      "suggested_key": {
        "default": "Alt+Shift+8"
      }
    },
    "go-to-9": {
      "description": "go to tab n9",
      "suggested_key": {
        "default": "Alt+Shift+9"
      }
    }
  },
  "background": {
    "scripts": [
      "foo.js"
    ]
  }
}

