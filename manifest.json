{
  "manifest_version": 2,
  "name": "tabmove",
  "version": "1.1",
  "description": "move current tab with alt+shit+n",
  "permissions": [
      "tabs",
      "activeTab"
  ],
  "commands": {
    "go-to-1": {
      "description": "move to the first index",
      "suggested_key": {
        "default": "Alt+Shift+1"
      }
    },
    "go-to-2": {
      "description": "swap with left tab",
      "suggested_key": {
        "default": "Alt+Shift+2"
      }
    },
    "go-to-3": {
      "description": "swap with right tab",
      "suggested_key": {
        "default": "Alt+Shift+3"
      }
    },
    "go-to-4": {
      "description": "move to the last index",
      "suggested_key": {
        "default": "Alt+Shift+4"
      }
    }
  },
  "background": {
    "scripts": [
      "foo.js"
    ]
  }
}

// vim: set ts=2 sts=2 sw=2 et:
